{"ast":null,"code":"import data from \"./data\";\nimport Discount from \"./Discount.vue\";\nexport default {\n  name: \"App\",\n\n  data() {\n    return {\n      menuNames: [\"HOME\", \"SHOP\", \"ABOUT\"],\n      dummydata: data,\n      modalState: false,\n      titleNum: 0\n    };\n  },\n\n  methods: {},\n  components: {\n    Discount: Discount\n  }\n};","map":{"version":3,"mappings":"AAiCA,OAAOA,IAAP,MAAiB,QAAjB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AACA,eAAe;EACbC,IAAI,EAAE,KADO;;EAEbF,IAAI,GAAG;IACL,OAAO;MACLG,SAAS,EAAE,CAAC,MAAD,EAAS,MAAT,EAAiB,OAAjB,CADN;MAELC,SAAS,EAAEJ,IAFN;MAGLK,UAAU,EAAE,KAHP;MAILC,QAAQ,EAAE;IAJL,CAAP;EAMD,CATY;;EAUbC,OAAO,EAAE,EAVI;EAWbC,UAAU,EAAE;IACVP,QAAQ,EAAEA;EADA;AAXC,CAAf","names":["data","Discount","name","menuNames","dummydata","modalState","titleNum","methods","components"],"sourceRoot":"","sources":["/Users/gwondotojambo/Documents/vue_is_vue/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"blockBg\" v-if=\"modalState == true\">\n    <div class=\"whiteBg\">\n      <button @click=\"modalState = false\">X</button>\n      <Discount />\n      <img :src=\"dummydata[titleNum].image\" class=\"roomImg\" />\n      <h4>{{ dummydata[titleNum].title }}</h4>\n      <p>{{ dummydata[titleNum].content }}</p>\n    </div>\n  </div>\n\n  <div class=\"menu\">\n    <a v-for=\"menuName in menuNames\" :key=\"menuName\">{{ menuName }}</a>\n  </div>\n  <Discount />\n  <div v-for=\"(dummydatas, i) in dummydata\" :key=\"i\">\n    <img :src=\"dummydata[i].image\" class=\"roomImg\" />\n    <h4\n      @click=\"\n        modalState = true;\n        titleNum = dummydata[i].id;\n      \"\n    >\n      {{ dummydata[i].title }}\n    </h4>\n    <p>{{ dummydata[i].price }}</p>\n    <button @Click=\"dummydata[i].reportCount++\">허위매물 신고</button>\n    <span>신고횟수 : {{ dummydata[i].reportCount }}</span>\n    <p></p>\n  </div>\n</template>\n\n<script>\nimport data from \"./data\";\nimport Discount from \"./Discount.vue\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      menuNames: [\"HOME\", \"SHOP\", \"ABOUT\"],\n      dummydata: data,\n      modalState: false,\n      titleNum: 0,\n    };\n  },\n  methods: {},\n  components: {\n    Discount: Discount,\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n\nbody {\n  margin: 0;\n}\ndiv {\n  box-sizing: border-box;\n}\n\n.menu {\n  background-color: darkslateblue;\n  padding: 15px;\n  border-radius: 5px;\n}\n\n.menu a {\n  color: white;\n  padding: 10px;\n}\n\n.roomImg {\n  width: 100%;\n  margin-top: 40px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}